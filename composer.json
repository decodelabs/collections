{
    "name": "decodelabs/collections",
    "description": "Reusable collection components",
    "type": "library",
    "keywords": [ "collection", "data", "structure" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/coercion": "^0.3.3",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/fluidity": "^0.3.6",
        "decodelabs/lucid-support": "^0.5.3"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "suggest": {
        "decodelabs/lucid": "Value sanitising and validation"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Collections\\": "src/Collections",
            "DecodeLabs\\PHPStan\\Collections\\": "src/PHPStan"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Collections\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.11.x-dev"
        },
        "effigy": {
            "exports": [
                "phpstan-extension.neon"
            ]
        },
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
